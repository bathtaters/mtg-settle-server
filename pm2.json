{
  "name"             : "my-project-server",
  "cwd"              : "./",
  "args"             : [],
  "script"           : "src/internal/server.js",
  "node_args"        : ["--harmony", " --max-stack-size=102400000"],
  "log_date_format"  : "YYYY-MM-DD HH:mm Z",
  "error_file"       : "/var/log/my-project/server.stderr.log",
  "out_file"         : "log/my-project-server.stdout.log",
  "pid_file"         : "pids/my-project-server.pid",
  "instances"        : 6,
  "min_uptime"       : "200s",
  "max_restarts"     : 10,
  "max_memory_restart": "500K",
  "exp_backoff_restart_delay": "100",
  "cron_restart"     : "1 0 * * *",
  "watch"            : false,
  "merge_logs"       : true,
  "exec_interpreter" : "node",
  "exec_mode"        : "fork",
  "autorestart"      : true,
  "stop_exit_codes"  : [0],
  "vizion"           : false
}