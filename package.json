{
  "name": "mtg-settle-server",
  "version": "0.1.1",
  "description": "Backend server for Magic the Gathering set guessing game",
  "main": "./built/engine/server.js",
  "scripts": {
    "dev": "nodemon -x ts-node ./engine/server.js",
    "start": "pm2 start ecosystem.config.js && pm2 monit",
    "stop": "pm2 delete ecosystem.config.js",
    "reload": "pm2 reload ecosystem.config.js && pm2 logs",
    "build": "tsc --build --clean && tsc --build",
    "trace-sync": "node --trace-sync-io ./engine/server.js",
    "inspect": "nodemon --inspect ./engine/server.js",
    "unlock-user": "node ./engine/utils/Users.unlock.js",
    "test": "jest /*/testing/",
    "test-api": "./src/testing/testApi.sh",
    "dev-cert": "openssl req -new -newkey rsa:4096 -x509 -sha256 -days 365 -nodes -out .cert.pem -keyout .key.pem"
  },
  "keywords": [],
  "author": "bathtaters",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/bathtaters/mtg-settle-server"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@journeyapps/sqlcipher": "^5.3.1",
    "@sentry/node": "^9.3.0",
    "@sentry/profiling-node": "^9.3.0",
    "connect-flash": "^0.1.1",
    "connect-sqlite3": "^0.9.13",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.0.1",
    "express": "4.21.2",
    "express-graceful-exit": "^0.5.2",
    "express-rate-limit": "^7.0.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "graphql": "^16.6.0",
    "hat": "^0.0.3",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "imagekit": "^5.0.1",
    "morgan": "^1.10.0",
    "object-path": "^0.11.8",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "pm2": "^5.2.0",
    "pug": "^3.0.2",
    "re2": "^1.17.7",
    "sqlite3": "^5.0.11",
    "tiny-csrf": "^1.1.3",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hat": "^0.0.4",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/object-path": "^0.11.1",
    "@types/passport": "^1.0.13",
    "@types/passport-http-bearer": "^1.0.38",
    "@types/passport-local": "^1.0.36",
    "@types/supertest": "^6.0.2",
    "jest": "^29.4.1",
    "mtggraphql": "^1.0.2",
    "nodemon": "^3.0.1",
    "supertest": "^7.0.0",
    "supertest-session": "^5.0.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
